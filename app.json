{
  "expo": {
    "scheme": "ReactLearning",
    "name": "Local Hive",
    "slug": "LocalHive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/local-hive-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#7928CA",
    "splash": {
      "image": "./assets/local-hive-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#7928CA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/local-hive-logo.png",
        "backgroundColor": "#7928CA"
      },
      "edgeToEdgeEnabled": true,
      "softwareNavBarColor": "transparent",
      "navigationBarColor": "transparent",
      "windowSoftInputMode": "adjustResize"
    },
    "web": {
      "favicon": "./assets/local-hive-logo.png"
    },
    "plugins": ["expo-router"],
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "local-hive"
      }
    }
  }
}
