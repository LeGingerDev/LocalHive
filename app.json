{
  "expo": {
    "scheme": "localhive",
    "name": "Local Hive",
    "slug": "LocalHive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/local-hive-logo.png",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#7928CA",
    "splash": {
      "image": "./assets/local-hive-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#7928CA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/local-hive-logo.png",
        "backgroundColor": "#7928CA"
      },
      "edgeToEdgeEnabled": true,
      "softwareNavBarColor": "transparent",
      "navigationBarColor": "transparent",
      "windowSoftInputMode": "adjustResize",
      "package": "com.legingerdev.LocalHive",
      "userInterfaceStyle": "automatic",
      "androidNavigationBar": {
        "visible": "immersive"
      }
    },
    "web": {
      "favicon": "./assets/local-hive-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3a0c123a-2465-4f90-9799-f29fbe0c4bd4"
      }
    },
    "owner": "legingerdev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3a0c123a-2465-4f90-9799-f29fbe0c4bd4"
    }
  }
}
